

class Solution {
public:
    vector<int> targetIndices(vector<int>& nums, int target) {
        vector<int> ans;
        // 1 2 5 2 3
        sort(nums.begin(), nums.end());
        // 1 2 2 3 5
        for(int i=0;i<nums.size(); i++){
            if(nums[i] == target){
                ans.push_back(i);
            }
        }

       


     return ans;   
    }
};

