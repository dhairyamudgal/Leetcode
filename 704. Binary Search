class Solution {
public:
    int search(vector<int>& nums, int target) {
        int n = nums.size();
        int ans = -1;
        int s = 0;
        int e = n-1;

        while(s<=e){
            int m = s+(e-s)/2;

            if(nums[m] == target) return m;
            else if(nums[m] > target) e=m-1;
            else s = m+1;
        }

        return ans;
        
    }
};
